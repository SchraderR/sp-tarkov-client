{
  "name": "sp-tarkov-client",
  "version": "0.2.0",
  "description": "SP-EFT Manager",
  "author": {
    "name": "Schrader",
    "url": "https://github.com/SchraderR"
  },
  "main": "app/main.js",
  "private": true,
  "auto-changelog": {
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false
  },
  "scripts": {
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "installDependencies": "npm install && cd ./app && npm install",
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "ng:serve": "ng serve -c web",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "web:build": "npm run build -- -c web-production",
    "electron": "electron",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "17.0.8",
    "@angular/cdk": "17.0.4",
    "@angular/common": "17.0.8",
    "@angular/compiler": "17.0.8",
    "@angular/core": "17.0.8",
    "@angular/forms": "17.0.8",
    "@angular/language-service": "17.0.8",
    "@angular/material": "17.0.4",
    "@angular/platform-browser": "17.0.8",
    "@angular/platform-browser-dynamic": "17.0.8",
    "@angular/router": "17.0.8",
    "@fontsource/roboto": "^4.5.8",
    "@ngneat/transloco": "4.2.2",
    "@types/fast-levenshtein": "0.0.4",
    "electron-builder": "24.9.1",
    "fastest-levenshtein": "1.0.16",
    "karma-coverage": "2.2.1",
    "material-symbols": "0.14.3",
    "ngx-joyride": "2.5.0",
    "rxjs": "7.8.1",
    "tslib": "^2.5.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "17.0.0",
    "@angular-devkit/build-angular": "17.1.1",
    "@angular-eslint/builder": "17.1.1",
    "@angular-eslint/eslint-plugin": "17.1.1",
    "@angular-eslint/eslint-plugin-template": "17.1.1",
    "@angular-eslint/schematics": "17.1.1",
    "@angular-eslint/template-parser": "17.1.1",
    "@angular/cli": "17.0.8",
    "@angular/compiler-cli": "17.0.8",
    "@ngneat/spectator": "16.0.0",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@types/jasmine": "4.3.1",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "auto-changelog": "*",
    "conventional-changelog-cli": "2.2.2",
    "electron": "28.1.0",
    "electron-debug": "3.2.0",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.53.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "44.2.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "font-awesome": "4.7.0",
    "jasmine-core": "4.6.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.2",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "7.3.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "node-polyfill-webpack-plugin": "2.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.1",
    "ts-node": "10.9.2",
    "typescript": "~5.2.2",
    "wait-on": "7.2.0",
    "webdriver-manager": "12.1.9"
  }
}
