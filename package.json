{
  "name": "sp-tarkov-client",
  "version": "0.3.2",
  "description": "SP-EFT Manager",
  "author": {
    "name": "Schrader",
    "url": "https://github.com/SchraderR"
  },
  "main": "app/main.js",
  "private": true,
  "auto-changelog": {
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false
  },
  "scripts": {
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "installDependencies": "npm install && cd ./app && npm install",
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "ng:serve": "ng serve -c web",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:prod": "npm run build -- -c production",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "^18.2.14",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.14",
    "@angular/compiler": "^18.2.14",
    "@angular/core": "^18.2.14",
    "@angular/forms": "^18.2.14",
    "@angular/language-service": "^18.2.14",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^18.2.14",
    "@angular/platform-browser-dynamic": "^18.2.14",
    "@angular/router": "^18.2.14",
    "@fontsource/roboto": "^5.0.12",
    "@jsverse/transloco": "^7.4.0",
    "@types/fast-levenshtein": "0.0.4",
    "electron-builder": "24.13.3",
    "fastest-levenshtein": "1.0.16",
    "material-symbols": "^0.17.4",
    "ngx-joyride": "2.5.0",
    "ngx-pipes": "3.2.2",
    "rxjs": "7.8.1",
    "tslib": "^2.5.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^18.0.0",
    "@angular-eslint/builder": "^18.4.3",
    "@angular-eslint/eslint-plugin": "^18.4.3",
    "@angular-eslint/eslint-plugin-template": "^18.4.3",
    "@angular-eslint/schematics": "^18.4.2",
    "@angular-eslint/template-parser": "^18.4.3",
    "@angular/build": "^18.2.21",
    "@angular/cli": "^18.2.21",
    "@angular/compiler-cli": "^18.2.14",
    "@ngneat/spectator": "^18.0.2",
    "@types/fast-levenshtein": "0.0.4",
    "@types/jasmine": "5.1.4",
    "@types/jasminewd2": "2.0.13",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "auto-changelog": "*",
    "electron": "30.0.3",
    "electron-debug": "3.2.0",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.57.0",
    "font-awesome": "4.7.0",
    "jasmine-core": "5.1.2",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.3",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-electron": "7.3.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "node-polyfill-webpack-plugin": "3.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "typescript": "~5.5.4",
    "wait-on": "7.2.0"
  }
}
